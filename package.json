{
  "name": "pwa-boilerplate",
  "description": "PWA Boilerplate",
  "version": "0.1.0",
  "dependencies": {
    "@svgr/webpack": "4.3.3",
    "express": "^4.17.1",
    "html-react-parser": "^0.14.0",
    "next": "^9.5.3",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.26.10",
    "typescript": "~3.7.2",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "devDependencies": {
    "@types/css-modules": "^1.0.0",
    "@types/express": "^4.17.8",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^1.6.1",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "scripts": {
    "development": "NODE_ENV=development nodemon --watch server -e ts,json --exec 'ts-node -r tsconfig-paths/register --project tsconfig.server.json' server",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json --transpile-only -r tsconfig-paths/register .dist/index.js",
    "build": "npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
